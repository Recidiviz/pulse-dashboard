{
  "extends": ["react-app", "airbnb", "plugin:prettier/recommended"],
  "rules": {
    "react/jsx-filename-extension": ["error", { "extensions": [".js"] }],
    "import/no-extraneous-dependencies": ["error", {"packageDir": "./"}],
    "no-new": 1
  },
  "env": {
    "browser": true,
    "jest": true
  }
}
